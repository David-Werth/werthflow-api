datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id     String @id @default(uuid())
  userId String @unique
  name   String
  tasks  String @db.Text

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
